{
    "name"         : "nod.st/nod",
    "version"      : "0.1",
    "type"         : "cakephp-plugin",
    "keywords"     : ["cakephp", "nod", "admin", "plugin"],
    "repositories" : [
        {
            "type": "vcs",
            "url": "git@github.com:JacopKane/Cake-ClientRedirect.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:JacopKane/Cake-ControllersList.git"
        },
        {
            "type"      : "package",

                "name"      : "rhym/cake_strap",
                "version"   : "dev-master",
                "dist"      : {
                    "url": "https://github.com/Rhym/cakeStrap/archive/master.zip",
                    "type": "zip"
                },
                "minimum-stability": "dev",
                "extra": {
                    "installer-paths": {
